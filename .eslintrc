{
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"node": true,
		"jest": true
	},
	"extends": "eslint:recommended",
	"rules": {
		"comma-dangle": ["error", "always-multiline"],
		"object-curly-spacing": ["error", "always"]
	}
}