{
	"name": "generator-xes-project",
	"version": "0.0.1",
	"description": "yo generator for setting up projects",
	"author": "Xesenix",
	"files": [
		"generators",
		"helpers",
		"validators"
	],
	"keywords": [
		"yeoman-generator",
		"webpack",
		"babel",
		"typescript",
		"karma",
		"cypress",
		"docker"
	],
	"scripts": {
		"pretest": "eslint .",
		"precommit": "lint-staged",
		"test": "jest"
	},
	"jest": {
		"testEnvironment": "node",
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/templates/"
		]
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		],
		"*.json": [
			"git add"
		]
	},
	"eslintConfig": {
		"extends": [
			"xo"
		],
		"env": {
			"jest": true,
			"node": true
		}
	},
	"license": "ISC",
	"dependencies": {
		"jest": "^26.6.3"
	},
	"devDependencies": {
		"coveralls": "^3.1.0",
		"eslint": "^7.18.0",
		"eslint-config-standard": "^16.0.2",
		"eslint-config-xo": "^0.34.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^5.0.0",
		"husky": "^4.3.8",
		"lint-staged": "^10.5.3",
		"yeoman-assert": "^3.1.1",
		"yeoman-test": "^4.0.0"
	}
}
