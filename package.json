{
  "name": "generator-xes-project",
  "version": "0.0.1",
  "description": "yo generator for setting up projects",
  "author": "Xesenix",
  "files": [
    "generators",
    "lib"
  ],
  "keywords": [
    "yeoman-generator",
    "webpack",
    "babel",
    "typescript",
    "karma",
    "cypress",
    "docker"
  ],
  "scripts": {
    "pretest": "eslint --report-unused-disable-directives --fix .",
    "precommit": "lint-staged",
    "postinstall": "husky install",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "generators/**/*.js"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/templates/"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "errorOnDeprecated": true,
    "notify": true,
    "verbose": true
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.23.0",
    "yeoman-generator": "^5.4.0"
  },
  "devDependencies": {
    "@types/assert-plus": "^1.0.4",
    "@types/ini": "^1.3.31",
    "@types/node-notifier": "^8.0.1",
    "@types/prettier": "^2.4.1",
    "@types/yeoman-generator": "^5.2.7",
    "assert-plus": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-xo": "^0.37.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^7.0.2",
    "ini": "^2.0.0",
    "jest": "^27.3.1",
    "jsonlint": "^1.6.3",
    "lint-staged": "^12.2.2",
    "markdownlint-cli": "^0.26.0",
    "node-notifier": "^10.0.0",
    "package-json": "^7.0.0",
    "prettier": "^2.4.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-environment": "^3.6.0",
    "yeoman-test": "^6.2.0"
  }
}
